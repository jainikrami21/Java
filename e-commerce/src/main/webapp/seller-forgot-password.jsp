					<div id="success"></div>
					<form action="SellerController" name="sentmessage" method="post">
						<div class="control-group">
							<input type="email" class="form-control" id="email" name="email"
								placeholder="Your Email" required="required" data-validation-required-message="Please Enter Your Email"/>
							<p class="help-block text-danger"></p>
						</div>
						<div>
							<button class="btn btn-primary py-2 px-4" type="submit"
							 name="action" value="GET OTP" id="sendMessageButton">GET OTP
							 </button>
						</div>
					</form>
					<%String msg = (String)request.getAttribute("msg"); %>
					<%if(msg!=null){ %>
						<%out.print(msg); %>
					<%}%>
				</div>
			</div>
		</div>
		
