    <div class="container">
	<table class="table">
		<thead>
			<tr>
				<th scope="col">Product Name</th>
				<th scope="col">Product Price</th>
				<th scope="col">Product Quantity</th>
				<th scope="col">Total</th>
			</tr>
		</thead>
		<tbody>
		<%List<Cart> list = CartDao.getCartByCusId(s.getId()); %>
		<%int net_price = 0; %>
		<%for(Cart c1:list){ %>
				<%net_price = net_price + c1.getTotal(); %>
			<tr>
				<th scope="row"><%=c1.getPname() %></th>
				<td><%=c1.getPprice() %></td>
				<td>
					<form action="CartController" method="post">
						<input type="hidden" name="cid" value="<%=c1.getCid()%>">
						<input type="number" min="1" max="10" value="<%=c1.getPqty()%>" name="pqty" onchange="this.form.submit();">
					</form>
				</td>
				<td><%=c1.getTotal() %></td>
			</tr>
			<%} %>
			
		</tbody>
	</table>
		<form method="post" action="payment.jsp?amount=<%=net_price%>&cid=<%=s.getId()%>">
		<table border="1">
			<tbody>
				<tr>
					<th>S.No</th>
					<th>Label</th>
					<th>Value</th>
				</tr>
				
				<tr>
					<td>1</td>
					<td><label>txnAmount*</label></td>
					<td><input title="TXN_AMOUNT" tabindex="10"
						type="text" name="TXN_AMOUNT"
